        <br><h1>Questions</h1><hr>
        <br><a href="/questions" class="btn btn-primary">Make questions</a><br>
        <% questions.forEach(question => { %>
            <br>
            <div class="card">
                <div class="card-body">
                    <h3><%= question.title %></h3>
                </div>
                <div class="card-footer">   
                    <button class="btn btn-primary">Response</button>
                </div>
            </div>

        <% }) %>
    </div>
</body>
